{
  "name": "angular-i18n",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "extract-i18n": "ng extract-i18n angular-i18n --format xlf --output-path src/locale/ && ng run angular-i18n:xliffmerge",
    "start-en": "ng serve --configuration=en -o",
    "start-es": "ng serve --configuration=es -o"
  },
  "private": true,
